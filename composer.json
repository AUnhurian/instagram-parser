{
    "name": "aunhurian/instagram-parser",
    "description": "PHP parser to consume the instagram feed.",
    "type": "php-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii Un.",
            "email": "elwahab@ukr.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "autoload": {
        "psr-4": {
            "Mineur\\InstagramParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mineur\\InstagramParserTest\\": "test/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "fzaninotto/faker": "^1.5",
        "mockery/mockery": "^1.6",
        "symfony/var-dumper": "^6.0"
    }
}
